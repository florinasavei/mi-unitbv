<Window x:Class="WPFin30Mins.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFin30Mins"
        xmlns:models="clr-namespace:WPFin30Mins.Models"
        mc:Ignorable="d"
        Title="MainWindow" d:DesignHeight="200" d:DesignWidth="200" Cursor="Pen" Loaded="Window_Loaded">
    <Window.Resources>
        <DataTemplate DataType="{x:Type models:MyModelObject}" >
            <DockPanel Background="DarkGray"></DockPanel>
        </DataTemplate>
    </Window.Resources>
    <Grid x:Name="mainGridPannel">
        <StackPanel>
            <Button x:Name="button1" Content="{Binding Name}"></Button>
            <Button x:Name="button2" Content="{Binding Name}"></Button>
        </StackPanel>
    </Grid>
</Window>
